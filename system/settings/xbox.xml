<?xml version="1.0" encoding="utf-8" ?>
<settings>
  <section id="services">
    <category id="sntp" label="38601" help="38602">
      <group id="1">
        <setting id="services.timeserver" type="boolean" label="38600" help="">
          <level>1</level>
          <default>false</default>
        </setting>
        <setting id="services.timeserveraddress" type="string" label="38696" help="">
          <level>1</level>
          <default>pool.ntp.org</default>
          <control type="edit" format="string">
            <dependencies>
              <dependency type="enable" setting="services.timeserver">true</dependency>
            </dependencies>
          </control>
        </setting>
      </group>
    </category>
  </section>
  <section id="system">
    <category id="general">
      <group id="6">
        <setting id="myprograms.autoffpatch" type="boolean" label="38791" help="">
          <level>1</level>
          <default>false</default>
        </setting>
        <setting id="myprograms.gameautoregion" type="boolean" label="511" help="">
          <level>1</level>
          <default>false</default>
        </setting>
        <setting id="myprograms.ntscmode" type="integer" label="38766" help="">
          <level>1</level>
          <default>0</default> <!-- NTSC-M and NTSC-J -->
          <constraints>
            <options>
              <option label="38762">0</option> <!-- NTSC-M and NTSC-J -->
              <option label="38763">1</option> <!-- NTSC-M -->
              <option label="38764">2</option> <!-- NTSC-J -->
              <option label="38765">3</option> <!-- PAL-60 -->
            </options>
          </constraints>
          <control type="spinner" format="string">
            <dependencies>
              <dependency type="enable" setting="myprograms.gameautoregion">true</dependency>
            </dependencies>
          </control>
        </setting>
      </group>
      <group id="8">
        <setting id="myprograms.trainerpath" type="path" label="20003" help="">
          <level>1</level>
          <default>select folder</default>
          <heading>657</heading>
          <constraints>
            <writable>false</writable>
          </constraints>
          <control type="button" format="path" />
        </setting>
      </group>
      <group id="9">
        <setting id="myprograms.usedashpath" type="boolean" label="38720" help="">
          <level>1</level>
          <default>true</default>
        </setting>
        <setting id="myprograms.dashboard" type="string" label="38719" help="">
          <level>1</level>
          <default>C:\xboxdash.xbe</default>
          <control type="edit" format="string">
            <dependencies>
              <dependency type="enable" setting="myprograms.usedashpath">true</dependency>
            </dependencies>
          </control>
        </setting>
      </group>
    </category>
    <category id="lcd" label="38644" help="">
      <group id="1">
        <setting id="lcd.type" type="integer" label="38645" help="">
          <level>1</level>
          <default>0</default> <!-- LCD_TYPE_NONE -->
          <constraints>
            <options>
              <option label="351">0</option> <!-- LCD_TYPE_NONE -->
              <option label="38651">1</option> <!-- LCD_TYPE_LCD_HD44780 -->
              <option label="38652">2</option> <!-- LCD_TYPE_LCD_KS0073 -->
              <option label="38653">3</option> <!-- LCD_TYPE_VFD -->
            </options>
          </constraints>
          <control type="spinner" format="string" />
        </setting>
        <setting id="lcd.modchip" type="integer" label="38670" help="">
          <level>1</level>
          <default>0</default> <!-- MODCHIP_SMARTXX -->
          <constraints>
            <options>
              <option label="38671">0</option> <!-- MODCHIP_SMARTXX -->
              <option label="38672">1</option> <!-- MODCHIP_XENIUM -->
              <option label="38673">2</option> <!-- MODCHIP_XECUTER3 -->
            </options>
          </constraints>
          <control type="spinner" format="string">
            <dependencies>
              <dependency type="enable" setting="lcd.type" operator="!is">0</dependency>
            </dependencies>
          </control>
        </setting>
        <setting id="lcd.backlight" type="integer" label="463" help="">
          <level>1</level>
          <default>80</default>
          <constraints>
            <minimum>0</minimum>
            <step>5</step>
            <maximum>100</maximum>
            <formatlabel>14047</formatlabel>
          </constraints>
          <control type="spinner" format="string">
            <dependencies>
              <dependency type="enable" setting="lcd.type" operator="!is">0</dependency>
            </dependencies>
          </control>
        </setting>
        <setting id="lcd.contrast" type="integer" label="465" help="">
          <level>1</level>
          <default>100</default>
          <constraints>
            <minimum>0</minimum>
            <step>5</step>
            <maximum>100</maximum>
            <formatlabel>14047</formatlabel>
          </constraints>
          <control type="spinner" format="string">
            <dependencies>
              <dependency type="enable">
                <and>
                  <condition setting="lcd.type" operator="!is">0</condition> <!-- LCD_TYPE_NONE -->
                  <condition setting="lcd.modchip" operator="!is">2</condition> <!-- MODCHIP_XECUTER3 -->
                </and>
              </dependency>
            </dependencies>
          </control>
        </setting>
      </group>
      <group id="2">
        <setting id="lcd.disableonplayback" type="integer" label="38643" help="">
          <level>1</level>
          <default>0</default> <!-- LED_PLAYBACK_OFF -->
          <constraints>
            <options>
              <option label="106">0</option> <!-- LED_PLAYBACK_OFF -->
              <option label="13002">1</option> <!-- LED_PLAYBACK_VIDEO -->
              <option label="475">2</option> <!-- LED_PLAYBACK_MUSIC -->
              <option label="476">3</option> <!-- LED_PLAYBACK_VIDEO_MUSIC -->
            </options>
          </constraints>
          <control type="spinner" format="string">
            <dependencies>
              <dependency type="enable" setting="lcd.type" operator="!is">0</dependency>
            </dependencies>
          </control>
        </setting>
        <setting id="lcd.enableonpaused" type="boolean" label="38641" help="">
          <level>1</level>
          <default>true</default>
          <control>
            <dependencies>
              <dependency type="enable">
                <and>
                  <condition setting="lcd.type" operator="!is">0</condition> <!-- LCD_TYPE_NONE -->
                  <condition setting="lcd.disableonplayback" operator="!is">0</condition> <!-- LED_PLAYBACK_OFF -->
                </and>
              </dependency>
            </dependencies>
          </control>
        </setting>
      </group>
    </category>
  </section>
</settings>
